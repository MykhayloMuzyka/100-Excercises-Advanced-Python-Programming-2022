        "def calculate():\n",
        "    res = 0\n",
        "    for i in range(10, 100):\n",
        "        for j in range(i, 100):\n",
        "            if str(i * j) == str(i * j)[::-1] and i * j > res:\n",
        "                res = i * j\n",
        "    return res"
      ]
    },
