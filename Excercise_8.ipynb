        "def is_prime(num):\n",
        "    if num in (0, 1):\n",
        "        return False\n",
        "    for i in range(num // 2 + 1, 1, -1):\n",
        "        if num % i == 0:\n",
        "            return False\n",
        "    return True"
      ]
    }
  ]
